{
    "asciidoc.antora.showEnableAntoraPrompt": false,
    "[asciidoc]": {
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 120
    },
    "editor.rulers": [120],
    "editor.tabSize": 4,
    "cSpell.language": "ru,en",
    "cSpell.maxNumberOfProblems": 10000,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "cSpell.words": [
        "DATEADD", "DATEDIFF", "DECFLOAT",
        "GDSCODE",
		"LOCALTIME", "LOCALTIMESTAMP",
        "LPAD", "PSQL", "RPAD", "TRUNC",
        "SAVEPOINT", "SMALLINT", "SQLCODE", "SQLSTATE",
        "VARBINARY", "VARCHAR",
        "подзапрос", "подзапроса", "подзапросов", "подзапросом", "подзапросы", "подзапросами"
    ]
}
